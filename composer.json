{
    "name": "mydnic/laravel-form-blade-components",
    "description": "Collection of Blade Components for Forms",
    "keywords": [
        "laravel",
        "blade",
        "component",
        "form"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mydnic/laravel-form-blade-components",
    "authors": [
        {
            "name": "Cl√©ment Rigo",
            "email": "rigoclement@mydnic.be"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.4.1",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Mydnic\\FormBladeComponents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mydnic\\FormBladeComponents\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mydnic\\FormBladeComponents\\FormBladeComponentsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
